<owl:NamedIndividual rdf:about="phantolide">
  <rdf:type rdf:resource="http://scientificvariablesontology.org/svo#Matter"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">phantolide</rdfs:label>
  <svo:has_wikipedia_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Synthetic_musk</svo:has_wikipedia_page>
  <svo:has_wikidata_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.wikidata.org/wiki/Q3328283</svo:has_wikidata_page>
