  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">3-nitroaniline</rdfs:label>
  <svo:has_wikipedia_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/3-Nitroaniline</svo:has_wikipedia_page>
