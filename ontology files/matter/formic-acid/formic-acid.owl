  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">formic-acid</rdfs:label>
  <svo:has_wikipedia_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Formic_acid</svo:has_wikipedia_page>
