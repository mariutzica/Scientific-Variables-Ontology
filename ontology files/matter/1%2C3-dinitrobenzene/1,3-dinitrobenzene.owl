  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1,3-dinitrobenzene</rdfs:label>
  <svo:has_wikipedia_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Dinitrobenzene</svo:has_wikipedia_page>
