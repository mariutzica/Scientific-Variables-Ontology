:methlyglyoxal a svo:Matter,
        owl:NamedIndividual ;
    rdfs:label "methlyglyoxal"^^xsd:string ;
    svo:has_synonym "2-oxopropanal"^^xsd:string,
        "pyruvaldehyde"^^xsd:string ;
    svo:has_wikidata_page "https://www.wikidata.org/wiki/Q903881"^^xsd:string ;
