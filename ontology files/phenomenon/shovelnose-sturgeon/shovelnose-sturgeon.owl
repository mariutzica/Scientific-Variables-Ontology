  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">shovelnose-sturgeon</rdfs:label>
  <svo:has_wikipedia_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Shovelnose_sturgeon</svo:has_wikipedia_page>
