  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">treatment-plant</rdfs:label>
  <svo:has_wikipedia_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Water_treatment</svo:has_wikipedia_page>
