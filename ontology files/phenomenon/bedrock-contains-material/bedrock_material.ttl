@prefix : <http://scientificvariablesontology.org/svo/phenomenon/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix svo: <http://scientificvariablesontology.org/svo#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:bedrock-contains-material a svo:Phenomenon,
        owl:NamedIndividual ;
    rdfs:label "bedrock_material"^^xsd:string ;
    svo:has_containing_phenomenon :bedrock ;
    svo:has_primary_participant_phenomenon :material .

