  <rdf:type rdf:resource="http://scientificvariablesontology.org/svo#Phenomenon"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iceberg</rdfs:label>
  <svo:has_wikipedia_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Iceberg</svo:has_wikipedia_page>
  <svo:has_wikidata_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.wikidata.org/wiki/Q47568</svo:has_wikidata_page>
