  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">radiant_emittance</rdfs:label>
  <svo:has_wikipedia_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Radiant_exitance</svo:has_wikipedia_page>
