  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">magnetic_susceptibility</rdfs:label>
  <svo:has_wikipedia_page rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://en.wikipedia.org/wiki/Magnetic_susceptibility</svo:has_wikipedia_page>
