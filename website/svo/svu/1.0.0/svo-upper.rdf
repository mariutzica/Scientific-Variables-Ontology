  </rdf:Description>
  <rdf:Description rdf:about="http://www.geoscienceontology.org/svo/svu#MathematicalAbstraction">
    <rdfs:comment>A mathematical abstraction is any mathematical entity.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Mathematical Abstraction</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.geoscienceontology.org/svo/svu#Abstraction"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.geoscienceontology.org/svo/svu#hasRole">
