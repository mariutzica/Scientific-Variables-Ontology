  </rdf:Description>
  <rdf:Description rdf:about="http://www.geoscienceontology.org/svo/svu#Body">
    <rdfs:comment>A Body is something that has both a well-defined form and substance.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.geoscienceontology.org/svo/svu#SpatialPhenomenon"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Body</rdfs:label>
  </rdf:Description>
