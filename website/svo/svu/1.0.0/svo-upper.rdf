  <rdf:Description rdf:about="http://www.geoscienceontology.org/svo/svu#isDerivedFrom">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment>isDerivedFrom links more specific concepts to more generic concepts. It is a mechanism for linking Individuals in the ontology, bypassing explicitly defined taxonomic class hierarchies.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.geoscienceontology.org/svo/svu#Abstraction">
    <rdfs:label>Abstraction</rdfs:label>
    <rdfs:comment>An abstraction is an abstract entity, whether a purely mathematical construct or an abstract physical concept.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
